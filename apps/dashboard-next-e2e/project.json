{
  "name": "dashboard-next-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/dashboard-next-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "dependsOn": ["test-db:up"],
      "options": {
        "cypressConfig": "apps/dashboard-next-e2e/cypress.config.ts",
        "devServerTarget": "dashboard-next:serve:development",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "dashboard-next:serve:production"
        }
      }
    },
    "test-db:up": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "docker compose up --detach test-db"
          }
        ],
        "cwd": "{projectRoot}"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/dashboard-next-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["dashboard-next"]
}
