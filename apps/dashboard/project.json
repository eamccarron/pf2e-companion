{
  "name": "dashboard",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/dashboard",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/remix:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "command": "serve build/index.js",
        "outputPath": "dist/apps/dashboard"
      }
    },
    "serve": {
      "executor": "@nx/remix:serve",
      "options": {
        "port": 3000,
        "command": "npx remix-serve build/index.js",
        "cwd": "{projectRoot}"
      }
    },
    "serve:e2e": {
      "executor": "@nx/remix:serve",
      "options": {
        "port": 3000,
        "command": "npx remix-serve build/index.js > /dev/null",
        "cwd": "{projectRoot}"
      }
    },
    "serve:dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx remix dev --port 4200 -c 'remix-serve build/index.js'"
        ],
        "cwd": "{projectRoot}"
      }
    },
    "start": {
      "dependsOn": ["build"],
      "command": "remix-serve build",
      "options": {
        "cwd": "apps/dashboard"
      }
    },
    "typecheck": {
      "command": "tsc",
      "options": {
        "cwd": "apps/dashboard"
      }
    }
  }
}
